        "import pandas as pd\n",
        "import ast\n",
        "\n",
        "def display_raw_data_as_dataframe(raw_data_string: str, table_name: str = \"Table\", limit: int = 10) -> None:\n",
        "    \"\"\"\n",
        "    Parses a string representation of a list of tuples (raw data) and displays it\n",
        "    as a pandas DataFrame.\n",
        "\n",
        "    Args:\n",
        "        raw_data_string: A string representation of a list of tuples containing the data.\n",
        "        table_name: The name of the table the data is from (for printing context).\n",
        "        limit: The limit applied to the data retrieval (for printing context).\n",
        "    \"\"\"\n",
        "    if raw_data_string.startswith(\"Error\"):\n",
        "        print(raw_data_string)\n",
        "        return\n",
        "\n",
        "    try:\n",
        "        data_list = ast.literal_eval(raw_data_string)\n",
        "\n",
        "        # Attempt to get column names from schema for better DataFrame\n",
        "        # Note: This function only takes raw_data_string, so we cannot directly\n",
        "        # access db to get the schema here. We will use generic column names\n",
        "        # or rely on column names being implicitly known if the caller provides them later.\n",
        "        # For now, using generic names as a fallback.\n",
        "        column_names = []\n",
        "        if data_list:\n",
        "             # Use generic names if schema parsing failed or column count mismatch\n",
        "             column_names = [f\"Column_{i+1}\" for i in range(len(data_list[0]))]\n",
        "             print(f\"Warning: Cannot retrieve schema within this function. Using generic column names: {column_names}\")\n",
        "\n",
        "        # Create a pandas DataFrame\n",
        "        if data_list:\n",
        "             df = pd.DataFrame(data_list, columns=column_names)\n",
        "        else:\n",
        "             df = pd.DataFrame() # Empty DataFrame if data_list is empty\n",
        "\n",
        "\n",
        "        # Display the DataFrame\n",
        "        print(f\"--- Data from table '{table_name}' (first {limit} rows) ---\")\n",
        "        display(df)\n",
        "\n",
        "    except (ValueError, SyntaxError) as e:\n",
        "        print(f\"Error parsing the data string: {e}\")\n",
        "        print(\"Raw data string received:\")\n",
        "        print(raw_data_string)\n",
        "\n",
        "# Example usage (conceptual):\n",
        "# raw_data = retrieve_table_data.invoke(...)\n",
