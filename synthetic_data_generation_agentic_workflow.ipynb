        "from langchain.tools import tool\n",
        "\n",
        "@tool # addin new tool\n",
        "def retrieve_table_data(table_name: str, limit: int = 1000) -> str:\n",
        "    \"\"\"\n",
        "    Retrieves a specified number of rows from a given table using a SQL query.\n",
        "    Takes the table name and an optional limit (defaulting to 1000) as input.\n",
        "    Returns the result of the SQL query execution.\n",
        "    \"\"\"\n",
        "    query = f\"SELECT * FROM \\\"{table_name}\\\" LIMIT {limit};\"\n",
        "    try:\n",
        "        result = db.run(query)\n",
        "        return result\n",
        "    except Exception as e:\n",
        "        return f\"Error retrieving data from table {table_name}: {e}\"\n",
        "\n",
        "tools.append(retrieve_table_data)\n",
        "\n",
        "# Recreate the agent with the new tool\n",
        "agent = create_react_agent(llm, tools, prompt=system_message)"
