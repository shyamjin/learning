      "source": [
        "@tool\n",
        "def generate_synthetic_data_tool(table_name: str, num_samples: int) -> str:\n",
        "    \"\"\"\n",
        "    Generates a specified number of synthetic data samples for a given table.\n",
        "    Retrieves real data from the table, analyzes its structure, generates synthetic data\n",
        "    mimicking the real data's characteristics, and returns the synthetic data as a CSV string.\n",
        "    Takes the table name and the number of synthetic data samples to generate as input.\n",
        "    \"\"\"\n",
        "    # 1. Retrieve real data from the specified table\n",
        "    raw_data_string = retrieve_table_data.invoke({\"table_name\": table_name})\n",
        "\n",
        "    if raw_data_string.startswith(\"Error\"):\n",
        "        return raw_data_string # Return error message if data retrieval failed\n",
        "\n",
        "    # 2. Retrieve table schema to get column names and types\n",
        "    schema_string = \"\"\n",
        "    column_names = []\n",
        "    try:\n",
        "        pragma_schema_info = db.run(f\"PRAGMA table_info(\\\"{table_name}\\\");\")\n",
        "        # Check if the output is likely PRAGMA format list of tuples\n",
        "        if pragma_schema_info and isinstance(ast.literal_eval(pragma_schema_info), list) and all(isinstance(item, tuple) and len(item) >= 6 for item in ast.literal_eval(pragma_schema_info)):\n",
        "             schema_string = pragma_schema_info\n",
        "             # Extract column names here as well for DataFrame creation\n",
        "             schema_list = ast.literal_eval(pragma_schema_info)\n",
        "             column_names = [col_info[1] for col_info in schema_list]\n",
        "        else:\n",
        "             # Fallback to SELECT sql from sqlite_master if PRAGMA output is unexpected\n",
        "             create_table_schema = db.run(f\"SELECT sql FROM sqlite_master WHERE name='{table_name}';\")\n",
        "             if create_table_schema:\n",
        "                 # The result is a string representation of a list of tuples, e.g., \"[(..., CREATE TABLE ...)]\"\n",
        "                 schema_list_check = ast.literal_eval(create_table_schema)\n",
        "                 if schema_list_check and schema_list_check[0] and len(schema_list_check[0]) > 1:\n",
        "                     schema_string = schema_list_check[0][1] # Extract the CREATE TABLE string\n",
        "                     # Attempt to parse column names from CREATE TABLE string as well\n",
        "                     columns_match = re.search(r'\\((.*?)\\)', schema_string, re.DOTALL)\n",
        "                     if columns_match:\n",
        "                         columns_str = columns_match.group(1)\n",
        "                         column_defs = [col.strip() for col in columns_str.split(',')]\n",
        "                         column_names = [parts[0].strip('\"') for col_def in column_defs if (parts := col_def.split())]\n",
        "                     else:\n",
        "                        column_names = [] # Could not parse names from CREATE TABLE\n",
        "                 else:\n",
        "                     schema_string = str(create_table_schema) # Use the raw output if extraction fails\n",
        "                     column_names = [] # Could not parse names from raw output\n",
        "             else:\n",
        "                 schema_string = \"Schema not available.\" # If no schema found\n",
        "                 column_names = [] # No schema, no names\n",
        "\n",
        "    except Exception as e:\n",
        "        schema_string = f\"Error retrieving schema using PRAGMA or sqlite_master: {e}\"\n",
        "        column_names = []\n",
        "        print(schema_string) # Print error for debugging\n",
        "\n",
        "\n",
        "    # 3. Parse the raw data string into a pandas DataFrame\n",
        "    try:\n",
        "        data_list = ast.literal_eval(raw_data_string)\n",
        "    except (ValueError, SyntaxError) as e:\n",
        "        return f\"Error parsing raw data string for {table_name}: {e}\"\n",
        "\n",
        "    # Create DataFrame with extracted or generic column names\n",
        "    if data_list:\n",
        "        # If column names were not successfully extracted from schema, or mismatch, use generic names\n",
        "        if not column_names or (data_list and len(data_list[0]) != len(column_names)):\n",
