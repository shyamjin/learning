      "source": [
        "import pandas as pd\n",
        "import ast\n",
        "import re\n",
        "from io import StringIO\n",
        "\n",
        "def analyze_database_data(raw_data_string: str, schema_string: str) -> str:\n",
        "    \"\"\"\n",
        "    Analyzes the retrieved database data based on the provided schema string.\n",
        "\n",
        "    Args:\n",
        "        raw_data_string: A string representation of a list of tuples containing the data.\n",
        "        schema_string: A string representation of the table schema (CREATE TABLE statement or PRAGMA table_info result).\n",
        "\n",
        "    Returns:\n",
        "        A formatted string summarizing the data analysis including schema details.\n",
        "    \"\"\"\n",
        "    analysis_summary = \"Data Analysis Summary:\\n\"\n",
        "\n",
        "    column_names = []\n",
        "    column_info = {} # To store details like data type, nullable, etc.\n",
        "\n",
        "    # Attempt to parse schema_string from PRAGMA table_info format first\n",
        "    try:\n",
        "        schema_list = ast.literal_eval(schema_string)\n",
        "        if isinstance(schema_list, list) and all(isinstance(item, tuple) and len(item) >= 6 for item in schema_list):\n",
        "             # Looks like PRAGMA table_info output: [(cid, name, type, notnull, dflt_value, pk), ...]\n",
        "             column_names = [col_info[1] for col_info in schema_list]\n",
        "             for col_info in schema_list:\n",
        "                  column_info[col_info[1]] = {\n",
        "                      \"type\": col_info[2],\n",
        "                      \"notnull\": bool(col_info[3]),\n",
        "                      \"pk\": bool(col_info[5])\n",
        "                  }\n",
        "        else:\n",
        "            # If not PRAGMA format, try parsing as CREATE TABLE statement\n",
        "            raise ValueError(\"Not PRAGMA table_info format\") # Jump to except block\n",
        "    except (ValueError, SyntaxError):\n",
        "         # Attempt to parse schema_string as CREATE TABLE statement\n",
        "         if schema_string:\n",
        "             columns_match = re.search(r'\\((.*?)\\)', schema_string, re.DOTALL)\n",
        "             if columns_match:\n",
        "                 columns_str = columns_match.group(1)\n",
        "                 column_defs = [col.strip() for col in columns_str.split(',')]\n",
        "                 for col_def in column_defs:\n",
        "                     parts = col_def.split()\n",
        "                     if parts:\n",
        "                         col_name = parts[0].strip('\"')\n",
        "                         column_names.append(col_name)\n",
        "                         col_details = {\"type\": \"Unknown\", \"notnull\": False, \"pk\": False}\n",
        "                         # Basic parsing of type and constraints\n",
        "                         if len(parts) > 1:\n",
        "                             col_details[\"type\"] = parts[1]\n",
        "                         if \"NOT\" in [p.upper() for p in parts] and \"NULL\" in [p.upper() for p in parts]:\n",
        "                             col_details[\"notnull\"] = True\n",
        "                         if \"PRIMARY\" in [p.upper() for p in parts] and \"KEY\" in [p.upper() for p in parts]:\n",
        "                             col_details[\"pk\"] = True\n",
        "                         column_info[col_name] = col_details\n",
        "             else:\n",
        "                 analysis_summary += \"Warning: Could not parse schema string.\\n\"\n",
        "         else:\n",
        "            analysis_summary += \"Warning: No schema string provided.\\n\"\n",
        "\n",
        "\n",
        "    # Parse the raw data string into a Python list of tuples\n",
        "    data_list = []\n",
        "    try:\n",
        "        data_list = ast.literal_eval(raw_data_string)\n",
        "    except (ValueError, SyntaxError) as e:\n",
        "        return f\"Error parsing raw data string: {e}\"\n",
        "\n",
        "    # Create a pandas DataFrame\n",
        "    if data_list:\n",
        "        # If column names were not successfully extracted from schema, use generic names\n",
        "        if not column_names or (data_list and len(data_list[0]) != len(column_names)):\n",
        "             if data_list:\n",
        "                 column_names = [f\"Column_{i+1}\" for i in range(len(data_list[0]))]\n",
        "                 analysis_summary += f\"Warning: Schema parsing failed or column count mismatch. Using generic column names: {column_names}\\n\"\n",
        "             else:\n",
        "                 return \"No data to analyze.\"\n",
        "\n",
        "        df = pd.DataFrame(data_list, columns=column_names)\n",
        "    else:\n",
        "        return \"No data to analyze.\"\n",
        "\n",
        "    # --- Schema Information ---\n",
        "    analysis_summary += \"\\nSchema Information:\\n\"\n",
        "    if column_info:\n",
        "        for col_name, details in column_info.items():\n",
        "             analysis_summary += f\"- Column: {col_name}, Type: {details['type']}, Not Null: {details['notnull']}, Primary Key: {details['pk']}\\n\"\n",
        "    elif column_names:\n",
        "         analysis_summary += \"Could not extract detailed schema information, but column names are:\\n\"\n",
        "         analysis_summary += \", \".join(column_names) + \"\\n\"\n",
        "    else:\n",
        "        analysis_summary += \"Could not retrieve schema information.\\n\"\n",
        "\n",
        "\n",
        "    # --- Data Characteristics Analysis ---\n",
        "    analysis_summary += \"\\nData Characteristics Analysis:\\n\"\n",
        "\n",
        "    # Data Types (inferred by pandas)\n",
        "    analysis_summary += \"\\nInferred Column Data Types (Pandas):\\n\"\n",
        "    analysis_summary += df.dtypes.to_string()\n",
        "\n",
        "    # Missing Values\n",
        "    analysis_summary += \"\\n\\nMissing Values Count:\\n\"\n",
        "    analysis_summary += df.isnull().sum().to_string()\n",
        "\n",
        "    # Descriptive Statistics for numerical columns\n",
        "    numerical_cols = df.select_dtypes(include=['number'])\n",
        "    if not numerical_cols.empty:\n",
        "        analysis_summary += \"\\n\\nDescriptive Statistics for Numerical Columns:\\n\"\n",
        "        analysis_summary += numerical_cols.describe().to_string()\n",
        "\n",
        "    # Unique values and counts for categorical/object columns\n",
        "    categorical_cols = df.select_dtypes(include=['object', 'category'])\n",
        "    if not categorical_cols.empty:\n",
        "        analysis_summary += \"\\n\\nUnique Values and Counts for Categorical/Object Columns:\\n\"\n",
        "        for col in categorical_cols.columns:\n",
        "            analysis_summary += f\"\\nColumn '{col}' ({df[col].nunique()} unique values):\\n\"\n",
        "            # Limit unique values shown for brevity\n",
        "            unique_counts = df[col].value_counts(dropna=False) # Include NaN counts\n",
        "            analysis_summary += unique_counts.head().to_string()\n",
        "            if len(unique_counts) > 5:\n",
        "                 analysis_summary += \"\\n  ...(and more)\"\n",
        "\n",
        "    # Attempt to infer patterns in string columns (basic)\n",
        "    analysis_summary += \"\\n\\nInferred String Patterns (Basic):\\n\"\n",
        "    for col in df.select_dtypes(include=['object']).columns:\n",
        "        non_null_values = df[col].dropna()\n",
        "        if not non_null_values.empty:\n",
        "            sample_value = non_null_values.iloc[0]\n",
        "            analysis_summary += f\"- Column '{col}':\\n\"\n",
        "            if isinstance(sample_value, str):\n",
        "                if \"@\" in sample_value and \".\" in sample_value:\n",
        "                     analysis_summary += \"  Potentially Email format.\\n\"\n",
        "                elif re.match(r'^\\+?[\\d\\s\\(\\)-]+$', sample_value):\n",
        "                     analysis_summary += \"  Potentially Phone Number format.\\n\"\n",
        "                elif re.match(r'^\\d{5}(-\\d{4})?$', sample_value):\n",
        "                     analysis_summary += \"  Potentially US Postal Code format.\\n\"\n",
        "                else:\n",
        "                     analysis_summary += f\"  Sample value format: '{sample_value}' (Length: {len(sample_value)})\\n\"\n",
        "            else:\n",
        "                 analysis_summary += f\"  Contains non-string values (e.g., {sample_value}).\\n\"\n",
        "        else:\n",
        "             analysis_summary += f\"- Column '{col}': Contains only missing values.\\n\"\n",
        "\n",
        "\n",
        "    # Basic Correlation Matrix for numerical columns\n",
        "    if not numerical_cols.empty and len(numerical_cols.columns) > 1:\n",
        "        analysis_summary += \"\\n\\nCorrelation Matrix (Numerical Columns):\\n\"\n",
        "        analysis_summary += numerical_cols.corr(numeric_only=True).to_string()\n",
        "\n",
        "    return analysis_summary\n",
        "\n",
        "# Now I need to call this function with both the raw data and the schema.\n",
        "# The schema needs to be retrieved first. I can use the sql_db_schema tool for this.\n",
        "# I will define a new tool that orchestrates data retrieval, schema retrieval, and analysis."
