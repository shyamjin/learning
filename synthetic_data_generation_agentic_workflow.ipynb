        "from langchain.tools import tool\n",
        "import pandas as pd\n",
        "import ast\n",
        "from io import StringIO\n",
        "\n",
        "@tool\n",
        "def visualize_data_comparison_tool(table_name: str, num_samples: int = 10) -> str:\n",
        "    \"\"\"\n",
        "    Retrieves original and generates synthetic data for a specified table,\n",
        "    then displays a side-by-side comparison.\n",
        "    Takes the table name and an optional number of samples (defaulting to 10) as input.\n",
        "    Returns a message indicating the comparison is displayed.\n",
        "    \"\"\"\n",
        "    # Retrieve original data\n",
        "    raw_original_data_string = retrieve_table_data.invoke({\"table_name\": table_name, \"limit\": num_samples})\n",
        "\n",
        "    if raw_original_data_string.startswith(\"Error\"):\n",
        "        return f\"Error retrieving original data for table {table_name}: {raw_original_data_string}\"\n",
        "\n",
        "    # Retrieve table schema to get column names for original data\n",
        "    column_names = []\n",
        "    try:\n",
        "        schema_info = db.run(f\"PRAGMA table_info(\\\"{table_name}\\\");\")\n",
        "        schema_list = ast.literal_eval(schema_info)\n",
        "        column_names = [col_info[1] for col_info in schema_list]\n",
        "    except Exception as e:\n",
        "        print(f\"Error retrieving or parsing schema for {table_name} table: {e}. Attempting fallback.\")\n",
        "        # Fallback to generic names if schema retrieval fails\n",
        "        original_data_list_check = ast.literal_eval(raw_original_data_string) if raw_original_data_string else []\n",
        "        if original_data_list_check:\n",
        "            column_names = [f\"Column_{i+1}\" for i in range(len(original_data_list_check[0]))]\n",
        "        print(f\"Warning: Using generic column names for table {table_name} (original data).\")\n",
        "\n",
        "\n",
        "    # Parse original data string into a pandas DataFrame\n",
        "    original_data_list = []\n",
        "    try:\n",
        "        original_data_list = ast.literal_eval(raw_original_data_string)\n",
        "    except (ValueError, SyntaxError) as e:\n",
        "        return f\"Error parsing original data string for {table_name}: {e}\"\n",
        "\n",
        "    if original_data_list:\n",
        "        if column_names and len(original_data_list[0]) == len(column_names):\n",
        "            original_df = pd.DataFrame(original_data_list, columns=column_names)\n",
        "        else:\n",
        "            # Fallback to generic names if schema parsing failed or column count mismatch\n",
        "            if not column_names or (original_data_list and len(original_data_list[0]) != len(column_names)):\n",
        "                 column_names = [f\"Column_{i+1}\" for i in range(len(original_data_list[0]))]\n",
        "                 print(f\"Warning: Column name count mismatch for {table_name} (original data). Using generic column names.\")\n",
        "            original_df = pd.DataFrame(original_data_list, columns=column_names)\n",
        "    else:\n",
        "        original_df = pd.DataFrame() # Empty DataFrame if no data\n",
        "\n",
        "\n",
        "    # Generate synthetic data as CSV string using the existing tool\n",
        "    synthetic_csv_string = generate_synthetic_data_llm_tool.invoke({\"table_name\": table_name, \"num_samples\": num_samples})\n",
        "\n",
        "    if synthetic_csv_string.startswith(\"Error\"):\n",
        "         # If synthetic data generation failed, display original data and the error\n",
        "         print(f\"--- Original {table_name} Data (first {num_samples} rows) ---\")\n",
        "         display(original_df)\n",
        "         return f\"Error generating synthetic data for table {table_name}: {synthetic_csv_string}\"\n",
        "\n",
        "\n",
        "    # Read synthetic CSV string into a pandas DataFrame\n",
        "    try:\n",
        "        synthetic_df = pd.read_csv(StringIO(synthetic_csv_string))\n",
        "    except Exception as e:\n",
        "         # If reading synthetic data failed, display original data and the error\n",
        "         print(f\"--- Original {table_name} Data (first {num_samples} rows) ---\")\n",
        "         display(original_df)\n",
        "         return f\"Error reading synthetic data CSV for table {table_name}: {e}\"\n",
        "\n",
        "\n",
        "    # Display side-by-side\n",
        "    print(f\"--- Original {table_name} Data (first {num_samples} rows) ---\")\n",
        "    display(original_df)\n",
        "\n",
        "    print(f\"\\n--- Generated Synthetic {table_name} Data ({num_samples} rows) ---\")\n",
        "    display(synthetic_df)\n",
        "\n",
        "    return f\"Displayed side-by-side comparison for table {table_name} with {num_samples} samples.\""
      ],
